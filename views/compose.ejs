<%- include("partials/header.ejs") -%> 
<h1>Compose</h1>
<form action="/" method="post">
    <div class="form-group">
        <label for="titleid">Title</label>
        <input type="text" class="form-control" id="titleid" name="composeTitle">
      </div>
      <div class="form-group">
        <label for="postid">Post</label>
        <textarea name="composePost" class="form-control" id="postid" cols="30" rows="5"></textarea>
      </div>
      <button type="submit" class="btn btn-primary" name="composeButton">Publish</button>
</form>
<%- include("partials/footer.ejs") -%> 